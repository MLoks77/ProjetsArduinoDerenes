#include <SoftI2CMaster.h>

#include <Wire.h>

const int rs = 12, en = 11, d4 = 5, d5 = 4, d6 = 3, d7 = 2;
LiquidCrystal lcd(rs, en, d4, d5, d6, d7);


void setup() {

lcd.begin(16,2); // taille du LCD 16x2
lcd.setCursor(1,0); // endroit o√π positionner le curseur
lcd.print("Le LCD MARCHE"); // Message
}

void loop() {


lcd.noDisplay; // eteint LCD
delay(500);
lcd.Display(); // rallume LCD
delay(500);
}
